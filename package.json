{
    "name": "edu.berkeley.eecs.lab11.webxr",
    "displayName": "Lab11 XR",
    "version": "1.0.0",
    "description": "An iOS environment for running WebXR content",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "ecosystem:cordova"
    ],
    "author": "Lab11",
    "license": "Apache-2.0",
    "dependencies": {
        "cordova-ios": "^5.0.1",
        "cordova-plugin-add-swift-support": "^2.0.2",
        "cordova-plugin-chrome-apps-common": "^1.0.7",
        "cordova-plugin-chrome-apps-iossocketscommon": "^1.0.2",
        "cordova-plugin-chrome-apps-sockets-udp": "^1.3.0",
        "cordova-plugin-compat": "^1.2.0",
        "cordova-plugin-statusbar": "^2.4.3",
        "cordova-plugin-webxr": "file:src/cordova-plugin-webxr",
        "cordova-plugin-whitelist": "^1.3.4",
        "es6-promise-plugin": "^4.2.2",
        "phonegap-plugin-media-stream": "^1.2.1"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "phonegap-plugin-media-stream": {},
            "cordova-plugin-webxr": {
                "WEBXR_AUTHORIZATION": "worldSensing",
                "CAMERA_USAGE_DESCRIPTION": "This app uses the camera for augmented reality",
                "GRAPHICS_FRAMEWORK": "sceneKit"
            },
            "cordova-plugin-chrome-apps-sockets-udp": {},
            "cordova-plugin-statusbar": {}
        },
        "platforms": [
            "ios"
        ]
    }
}
